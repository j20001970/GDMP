<?xml version="1.0" encoding="UTF-8" ?>
<class name="MediaPipeInteractiveSegmenter" inherits="MediaPipeVisionTask" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://raw.githubusercontent.com/godotengine/godot/master/doc/class.xsd">
	<brief_description>
		MediaPipe interactive segmenter.
	</brief_description>
	<description>
		MediaPipe vision task for performing interactive segmentation on the given image.
	</description>
	<tutorials>
	</tutorials>
	<methods>
		<method name="initialize">
			<return type="bool" />
			<param index="0" name="base_options" type="MediaPipeTaskBaseOptions" />
			<param index="1" name="output_confidence_masks" type="bool" default="true" />
			<param index="2" name="output_category_mask" type="bool" default="false" />
			<description>
				Initializes the interactive segmenter.
			</description>
		</method>
		<method name="segment">
			<return type="MediaPipeImageSegmenterResult" />
			<param index="0" name="image" type="MediaPipeImage" />
			<param index="1" name="keypoint" type="Vector2" default="Vector2(0, 0)" />
			<param index="2" name="scribble" type="PackedVector2Array" default="PackedVector2Array()" />
			<param index="3" name="region_of_interest" type="Rect2" default="Rect2(0, 0, 0, 0)" />
			<param index="4" name="rotation_degrees" type="int" default="0" />
			<description>
				Performs image segmentation on [param image].
				Either [param keypoint] or [param scribble] should be specified to create the reigon of interest (ROI) for segmentation.
			</description>
		</method>
	</methods>
</class>
